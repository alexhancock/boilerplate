{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "settings": {
      "ecmascript": 6
    },
    "rules": {
      "no-undef": 1,
      "no-unused-vars": ["error", { "vars": "all", "args": "after-used" }],
      "semi": 1,
      "no-extra-semi": 1
    }
}
